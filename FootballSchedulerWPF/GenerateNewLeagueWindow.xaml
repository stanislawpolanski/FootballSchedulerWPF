<Window x:Name="___No_Name_" x:Class="FootballSchedulerWPF.GenerateNewLeagueWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FootballSchedulerWPF"
        mc:Ignorable="d"
        Title="GenerateNewLeagueWindow" Height="450" Width="800">
    <Window.Resources>
        <CollectionViewSource x:Key="teamsViewSource" d:DesignSource="{d:DesignInstance {x:Type local:Teams}, CreateList=True}"/>
    </Window.Resources>
    <Grid HorizontalAlignment="Left" Height="255" Margin="152,87,0,0" VerticalAlignment="Top" Width="304">
        <ListBox HorizontalAlignment="Left" DataContext="{StaticResource teamsViewSource}" Height="160" Margin="75,10,0,0" VerticalAlignment="Top" Width="181" ItemsSource="{Binding }" x:Name="sourceTeamsListBox" DisplayMemberPath="Name"/>
    </Grid>
</Window>
